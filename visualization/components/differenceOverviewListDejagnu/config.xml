<?xml version="1.0" encoding="utf-8" ?>
<config>
  <!-- Basic component info -->
  <base>
    <name>Difference overview list</name>
    <identifier>difference-overview-list-dejagnu</identifier>
    <author>Filip Matys</author>
    <version>v1.0</version>
    <view-type>1</view-type>
    <folder>differenceOverviewListDejagnu</folder>
    <js>differenceOverviewListDejagnu.js</js>
    <description>List shows results side by side and highlights changes. Allows pagination and showing only changes.</description>
  </base>
  <!-- Supported plugins -->
  <supported-plugins>
    <plugin identifier="dejagnu-summary" />
  </supported-plugins>
  <!-- Template settings -->
  <setting-templates>
    <template identifier="page-size" label="Number of categories per page" type="1" default="4" required="true"></template>
  </setting-templates>
</config>